<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.StudentProfileEdit">
   <children>
      <fx:include source="navigationBar.fxml" />
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
         <children>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="First Name           ">
               <graphic>
                  <TextField fx:id="firstNameField" />
               </graphic>
            </Label>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Last Name            ">
               <graphic>
                  <TextField fx:id="lastNameField" />
               </graphic>
            </Label>
         </children>
         <VBox.margin>
            <Insets top="75.0" />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
         <children>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Email                   ">
               <graphic>
                  <TextField fx:id="emailField" />
               </graphic>
            </Label>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Student ID             ">
               <graphic>
                  <TextField fx:id="studentIDField" />
               </graphic>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
         <children>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Major                   ">
               <graphic>
                  <TextField fx:id="majorField" />
               </graphic>
            </Label>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Minor                    ">
               <graphic>
                  <TextField fx:id="minorField" />
               </graphic>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="200.0">
         <children>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Class Level         ">
               <graphic>
                  <TextField fx:id="classLevelField" />
               </graphic>
            </Label>
            <Label contentDisplay="RIGHT" graphicTextGap="20.0" text="Application Area   ">
               <graphic>
                  <TextField fx:id="applicationAreaField" />
               </graphic>
            </Label>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="saveBtn" mnemonicParsing="false" onAction="#saveBtnPressed" text="Save" />
         </children>
      </HBox>
   </children>
</VBox>
